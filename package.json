{
  "name": "obsidian-paper",
  "version": "0.1.0",
  "description": "Obsidian plugin for handwriting notes with Apple Pencil",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "build:copy": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production && mkdir -p ~/Library/Mobile\\ Documents/iCloud~md~obsidian/Documents/Personal/.obsidian/plugins/obsidian-paper && cp main.js manifest.json styles.css ~/Library/Mobile\\ Documents/iCloud~md~obsidian/Documents/Personal/.obsidian/plugins/obsidian-paper",
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "handwriting",
    "apple-pencil",
    "drawing"
  ],
  "author": "Aaron Hayman",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@eslint/json": "^0.14.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^16.11.6",
    "@types/rbush": "^4.0.0",
    "esbuild": "^0.25.5",
    "eslint": "^9.30.1",
    "eslint-plugin-obsidianmd": "^0.1.9",
    "globals": "^14.0.0",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.0",
    "jiti": "^2.6.1",
    "obsidian": "latest",
    "ts-jest": "^29.4.0",
    "tslib": "^2.4.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1"
  },
  "dependencies": {
    "fflate": "^0.8.2",
    "perfect-freehand": "^1.2.3",
    "rbush": "^4.0.1",
    "simplex-noise": "^4.0.3"
  }
}
